nt: space-between;\r\n    background-color: white;\r\n    & .hand-burger-menu {\r\n      font-size: 3rem;\r\n      font-weight: 900;\r\n      transform: rotate(90deg);\r\n      padding: 2rem;\r\n      cursor: pointer;\r\n    }\r\n    &-chiled1 {\r\n      & > img {\r\n        height: 100%;\r\n      }\r\n    }\r\n    &-chiled2 {\r\n      display: flex;\r\n      align-items: center;\r\n      list-style: none;\r\n      & > a {\r\n        color: $textColor;\r\n        text-decoration: none;\r\n        padding: 1rem;\r\n        font-size: 2rem;\r\n        margin: 1rem;\r\n        transition: all 0.4s ease-out;\r\n        &:hover {\r\n          color: rgb(244 94 94 / 95%);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n","scss\\mainsection.scss":".scroll-top-btn {\r\n  position: fixed;\r\n  bottom: 2rem;\r\n  right: -8rem;\r\n  font-size: 3rem;\r\n  transform: rotate(270deg);\r\n  background-color: black;\r\n  color: white;\r\n  border-radius: 50%;\r\n  padding: 0rem 1.5rem;\r\n  z-index: 60;\r\n  cursor: pointer;\r\n  box-shadow: 1px 1px 10px -5px black;\r\n  transition: all 1s ease-out;\r\n}\r\n.slide-in {\r\n  right: 3rem;\r\n  transition: all 0.5s ease-out;\r\n  &:hover {\r\n    background-color: white;\r\n    color: black;\r\n    box-shadow: 1px 1px 10px -5px black;\r\n  }\r\n}\r\n.main-nav-for-mobile {\r\n  position: fixed;\r\n  z-index: 80;\r\n  background: white;\r\n  width: 100%;\r\n  height: 90vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  list-style: none;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  & > a {\r\n    font-size: 3rem;\r\n    padding: 3rem;\r\n    color: black;\r\n    text-decoration: none;\r\n    transition: all 0.4s ease-out;\r\n    &:hover {\r\n      color: rgb(244 94 94 / 95%);\r\n    }\r\n  }\r\n}\r\n\r\nmain {\r\n  ///poster\r\n  & .main-poster {\r\n    height: 90vh;\r\n    width: 100%;\r\n    &-continer {\r\n      height: 100%;\r\n      position: relative;\r\n      & > img {\r\n        height: 100%;\r\n        width: 100%;\r\n        object-fit: cover;\r\n        position: absolute;\r\n      }\r\n      & > div {\r\n        position: absolute;\r\n        width: 100%;\r\n        height: 100%;\r\n        background-color: rgb(0 0 0 / 56%);\r\n        & > div {\r\n          position: absolute;\r\n          left: 15%;\r\n          top: 50%;\r\n          color: white;\r\n          transform: translate(-10%, -50%);\r\n          font-size: 3.8rem;\r\n          font-weight: 600;\r\n          line-height: 1.4;\r\n\r\n          & > a {\r\n            padding: 1rem;\r\n            background: white;\r\n            color: $textColor;\r\n            border-radius: 40px;\r\n            font-size: 1.9rem;\r\n            text-decoration: none;\r\n            transition: all 0.5s ease-out;\r\n            box-shadow: 1px 1px 10px -5px black;\r\n            &:hover {\r\n              background-color: $textColor;\r\n              color: white;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  /// about us\r\n  & .main-about-us {\r\n    margin: 12rem 2rem;\r\n\r\n    font-size: 2.2rem;\r\n    color: $textColor;\r\n\r\n    & > div {\r\n      text-align: center;\r\n      width: 70%;\r\n      margin: 0 auto;\r\n      & > h1 {\r\n        margin: 2rem 0rem;\r\n        font-size: 3rem;\r\n      }\r\n      & > h3 {\r\n        margin: 1rem 0;\r\n      }\r\n      & > p {\r\n        margin: 3rem 0;\r\n        & > span {\r\n          color: #ef6c77;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  & .main-products {\r\n    margin: 4rem 1rem;\r\n    font-size: 2rem;\r\n    &-title {\r\n      font-size: 2.5rem;\r\n      text-align: center;\r\n    }\r\n    &-grid-wapper {\r\n      & > div {\r\n        display: flex;\r\n        align-items: center;\r\n        margin: 4rem;\r\n        padding: 1rem;\r\n        & > .main-products-grid-wapper-content {\r\n          font-size: 1.8rem;\r\n          flex: 1;\r\n          padding: 2rem 4rem;\r\n          color: $textColor;\r\n          &